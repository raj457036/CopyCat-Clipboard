  "version": "0.2.0",
  "configurations": [
    {
      "name": "clipboard",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": ["--dart-define-from-file", "local/prod.json", "--verbose"]
    },
    {
      "name": "clipboard (profile mode)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "flutterMode": "profile",
      "args": ["--dart-define-from-file", "local/prod.json", "--verbose"]
    },
    {
      "name": "clipboard (release mode)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "flutterMode": "release",
      "args": ["--dart-define-from-file", "local/prod.json"]
    }
  ]
}
